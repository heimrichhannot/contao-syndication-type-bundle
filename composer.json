{
  "name": "heimrichhannot/contao-syndication-type-bundle",
  "type": "contao-bundle",
  "description": "This bundle brings an extendable syndication framework to contao.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.9",
    "heimrichhannot/contao-twig-support-bundle": "^0.2.16 || ^1.0",
    "psr/container": "^1.0 || ^2.0",
    "psr/link": "^1.0",
    "symfony/config": "^3.4|^4.4|^5.0",
    "symfony/event-dispatcher-contracts": "^1.0 || ^2.0 || ^3.0",
    "symfony/http-foundation": "^3.4|^4.4|^5.0",
    "symfony/string": "^5.0",
    "symfony/translation-contracts": "^1.0 || ^2.0 || ^3.0"
  },
  "conflict": {
    "heimrichhannot/contao-encore-bundle": "<1.10",
    "heimrichhannot/contao-reader-bundle": "<1.24"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\SyndicationTypeBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\SyndicationTypeBundle\\ContaoManager\\Plugin"
  }
}
