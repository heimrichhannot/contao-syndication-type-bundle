{
  "name": "heimrichhannot/contao-syndication-type-bundle",
  "type": "contao-bundle",
  "description": "This bundle contains a syndication config element type.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.2",
    "contao/core-bundle": "^4.4",
    "psr/link": "^1.0",
    "symfony/string": "^5.0",
    "symfony/http-foundation": "^3.4|^4.4",
    "symfony/translation": "^3.4|^4.4",
    "heimrichhannot/contao-twig-support-bundle": "^0.2.11"
  },
  "require-dev": {
    "contao/test-case": "1.1.*",
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "phpunit/phpunit": "^6.5",
    "phpunit/phpunit-mock-objects": "^4.0|^5.0",
    "phpunit/php-token-stream": "^1.4|^2.0|^3.0",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message-factory": "^1.0.2",
    "satooshi/php-coveralls": "^2.0",
    "symfony/phpunit-bridge": "^3.2"
  },
  "conflict": {
    "heimrichhannot/contao-encore-bundle": "<1.10",
    "heimrichhannot/contao-reader-bundle": "<1.24"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\SyndicationTypeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HeimrichHannot\\SyndicationTypeBundle\\Test\\": "tests/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\SyndicationTypeBundle\\ContaoManager\\Plugin"
  }
}
